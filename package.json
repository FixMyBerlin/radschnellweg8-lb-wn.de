  "license": "AGPL-3.0",
  "private": true,
  "keywords": [],
  "scripts": {
    "start": "gatsby develop --open",
    "mobile": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build-serve": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "updatePackages": "npx taze major --write",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d') [skip ci]\" --body \"\"",
    "check": "npm run type-check && npm run lint && npm run format"
  },
