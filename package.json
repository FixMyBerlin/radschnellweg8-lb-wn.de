{
  "name": "rsv.landingpage",
  "version": "0.0.1",
  "license": "GNU AGPL",
  "scripts": {
    "dev": "astro dev",
    "predev": "bun run ./shared/scripts/pre-dev.ts",
    "start": "echo \"Use npm run dev instead\"",
    "build": "astro build",
    "build:local": "npm run predev && ASTRO_OUTPUT_MODE=static astro build",
    "preview": "ASTRO_OUTPUT_MODE=static astro preview --open",
    "build:node": "npm run predev && ASTRO_OUTPUT_MODE=server ASTRO_USE_NODE_ADAPTER=true astro build",
    "preview:node": "ASTRO_OUTPUT_MODE=server ASTRO_USE_NODE_ADAPTER=true astro preview --open",
    "astro": "astro",
    "generate-types": "astro sync --verbose --force && npm run generate-types:highlightError",
    "generate-types:highlightError": "echo -e '\\033[47;30mSHOW TYPES `any`\\033[0m' && cat ./.astro/content.d.ts | grep -B 1 ': any'",
    "generate-types:full": "astro sync --verbose && npm run generate-types:highlightError && echo -e '\\033[47;30mSHOW FULL GENERATED TYPE FILE\\033[0m' && cat ./.astro/content.d.ts",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint '{src,config,keystatic,shared}/**/*.+(ts|js|tsx|astro)' --fix",
    "format": "prettier --ignore-path .gitignore '{src,config,keystatic,shared}/**/*.+(ts|js|tsx|astro)' --write",
    "format:all": "prettier --ignore-path .gitignore '{src,config,keystatic,shared}/**/*.+(ts|js|tsx|astro|mdx)' --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "updatePackages:major": "npx taze major --includeLocked --write && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install",
    "bleach": "rm -rf node_modules/.astro && rm -rf node_modules/.bin && rm -rf node_modules/.vite && rm -rf .astro/ && rm -rf .netlify/ && rm -rf dist/ && npm install && npm run bleach:info",
    "bleach:info": "echo \"\u001b[43m\u001b[30m(!) Also delete the browser cache, see https://github.com/vitejs/vite/discussions/17738#discussioncomment-10158746\u001b[0m\"",
    "bleach:all": "rm -rf node_modules/ && npm run bleach",
    "bleach:full": "rm package-lock.json && npm run bleach:all",
    "prepare": "husky"
  },
  "browserslist": [
    "defaults",
    "last 2 IE major versions",
    "last 4 Edge major versions"
  ],
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.1.0",
    "@astrojs/netlify": "^6.2.2",
    "@astrojs/node": "^9.1.3",
    "@astrojs/react": "^4.2.1",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/ts-plugin": "^1.10.4",
    "@fontsource-variable/red-hat-display": "^5.2.5",
    "@headlessui/react": "^2.2.0",
    "@headlessui/tailwindcss": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@keystatic/astro": "^5.0.6",
    "@keystatic/core": "^0.5.46",
    "@tailwindcss/vite": "^4.0.9",
    "@turf/turf": "^7.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "astro": "^5.4.2",
    "astro-heroicons": "^2.1.5-1",
    "astro-matomo": "^1.8.0",
    "clsx": "^2.1.1",
    "maplibre-gl": "^5.2.0",
    "react": "^19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.0.0",
    "react-map-gl": "^8.0.1",
    "recharts": "^2.15.1",
    "rehype-external-links": "^3.0.0",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "latest",
    "@types/node": "^22.13.9",
    "@typescript-eslint/parser": "^8.26.0",
    "colors": "^1.4.0",
    "eslint": "^9.21.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
