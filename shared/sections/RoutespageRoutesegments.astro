---
import { getCollection } from 'astro:content'
import RoutespageRoutesegmentsSegment from './RoutespageRoutesegmentsSegment.astro'

const unsortedSegments = await getCollection('routesegments')
const segments = unsortedSegments.sort((a, b) => a.data.position - b.data.position)
export type Segment = (typeof unsortedSegments)[number]
---

{
  segments.map((route) => {
    return <RoutespageRoutesegmentsSegment route={route} />
  })
}
